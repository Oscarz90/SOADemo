<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions
     name="ValidacionTarjetaCreditoService"
     targetNamespace="http://oracle.com/sca/soapservice/ValidacionTarjetaCredito/ValidacionTarjetaCredito/ValidacionTarjetaCreditoService"
     xmlns:tns="http://oracle.com/sca/soapservice/ValidacionTarjetaCredito/ValidacionTarjetaCredito/ValidacionTarjetaCreditoService"
