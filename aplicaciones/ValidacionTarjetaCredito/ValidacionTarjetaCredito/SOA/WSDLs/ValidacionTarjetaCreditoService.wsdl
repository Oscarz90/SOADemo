        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://www.example.org" schemaLocation="../Schemas/ValidacionTarjetaCreditoMensajes.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="requestMessage">
        <wsdl:part name="Peticion" element="inp1:Peticion"/>
    </wsdl:message>
    <wsdl:message name="replyMessage">
        <wsdl:part name="Respuesta" element="inp1:Respuesta"/>
    </wsdl:message>
    <wsdl:portType name="execute_ptt">
        <wsdl:operation name="ValidaTarjetaCredito">
            <wsdl:input message="tns:requestMessage"/>
